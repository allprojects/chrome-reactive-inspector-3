{
  "manifest_version": 2,
  "name": "Chrome Reactive Inspector",
  "version": "2.0",
  "description": "This is an extension to devtool for debugging Reactive JS libraries like Bacon.JS , RX-JS Version 5 ",
  "author": "Waqas Abbas and Pradeep Baradur with supervision from Dr.Guido Salvaneschi at TU - Darmstadt",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": [
      "helpers/comm_api_background.js",
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "libraries/jquery.js",
        "content-script-start.js",
        "helpers/comm_api_content-script.js",
        "jalangi-framework.js",
        "libraries/underscore.js",
        "Bacon.js",
        "Rx.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content-script-end.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "icons": {
    "128": "icon.png"
  },
  "web_accessible_resources": [
    "panel.js"
  ],
  "options_page": "options/options.html"
}
