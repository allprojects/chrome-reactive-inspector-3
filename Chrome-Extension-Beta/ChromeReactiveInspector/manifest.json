{
  "manifest_version": 3,
  "name": "Chrome Reactive Inspector",
  "version": "3.0.1",
  "description": "This extension enables developers to debug reactive web applications written in Rx.js 5 or Bacon.js. It adds a new panel to the Chrome DevTools that interacts with the inspected page.",
  "author": "Waqas Abbas, Pradeep Baradur and Benedikt Gross with supervision from Dr.Guido Salvaneschi at TU - Darmstadt",
  "devtools_page": "panel/devtools.html",
  "background": {
    "scripts": [
      "background/background-communication.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "libraries/jquery-1.12.4.min.js",
        "content-scripts/content-scripts-start.js",
        "content-scripts/content-script-communication.js",
        "content-scripts/jalangi-framework.js",
        "libraries/underscore-min.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ],
  "icons": {
    "128": "resources/icon.png"
  },
  "web_accessible_resources": [
    "panel/panel.js"
  ],
  "options_page": "options/options.html"
}
